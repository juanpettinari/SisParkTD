
@{
    ViewBag.Title = "Ingresar Vehiculo";
}

<h2>Ingresar Vehiculo</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <hr />
        <div class="form-group">
            @Html.Label("Patente", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.Editor("patente", new { htmlAttributes = new { @class = "form-control", autocomplete="off" }})
                <div class="text-danger">
                    @ViewBag.errorMessage
                </div>
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Ingresar" class="btn btn-default" />
            </div>
        </div>
    </div>
}
