@model SisParkTD.Models.Ticket

@{
    ViewBag.Title = "Re-Imprimir Ticket";
}

<h2>Re-Imprimir Ticket</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Tickets</h4>
        <hr />

        <div class="form-group">
            @Html.Label("Ticket")
            <div>
                @Html.Editor("ticketId")
                <div class="text-danger">
                    @ViewBag.errorNoExisteTicket
                </div>
            </div>
        </div>

        <div class="form-group">
            <div>
                <input type="submit" value="Re-Imprimir" class="btn btn-default" />
            </div>
        </div>
    </div>
}